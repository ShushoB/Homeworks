#1

num = int(input("Enter a number: ")) 
if num % 2 != 0: 
    print("You entered an odd number. ") 
else: 
    print("You entered an even number. ")
    
#2

ch = input("Enter a Character : ")
vowels = [ 'a', 'A', 'e', 'E', 'u', 'U', 'i', 'I', 'o', 'O']
if ch  in vowels:
    print(ch, "is a vowel")
else:
    print(ch, "is a consonant")
    
